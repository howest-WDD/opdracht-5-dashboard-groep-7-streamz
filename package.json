  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "sass": "^1.56.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  },
